<div class='container mb-5'>
  <div class='row my-3'>
    <h3><%= @user.handle_name %>のページ</h3>
  </div>

  <div class='row'>
    <div class='col-md-4'>
      <%= render 'public/users/user_info', user:@user %>
    </div>
    <div class='col-md-7 '>
      <% @itineraries.each do |itinerary| %>
        <%= render 'public/itineraries/itineraries', itinerary:itinerary %>
      <% end %>
    </div>
  </div>
</div>